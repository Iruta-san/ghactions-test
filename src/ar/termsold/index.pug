extends ../../pug/base

prepend vars
	- title = 'اتفاقية المستخدم | ROYALFUT'
	- description = 'اتفاقية معالجة البيانات الشخصية | ROYALFUT'

append vars
	- html.attrs.lang = 'ar'
	- html.attrs.dir = 'rtl'
	- body.attrs = {"data-barba": "wrapper"}
	- meta.viewport = 'width=device-width, initial-scale=1.0, maximum-scale=1.0, viewport-fit=cover'
	- link.icon384x384 = '/android-chrome-384x384.png'
	- link.icon192x192 = '/android-chrome-192x192.png'
	- link.appleTouchIcon180x180 = '/apple-touch-icon.png'
	- link.icon = '/favicon.ico'
	- link.icon32x32 = '/favicon-32x32.png'
	- link.icon16x16 = '/favicon-16x16.png'
	- link.manifest = '/site.webmanifest'
	- meta.msapplicationTileImage = '/mstile-150x150.png'
	- link.canonical = '/ar/termsold/'

block hrefLang
	link(rel="alternate" hreflang="ar-SA" href="https://royalfut.com/ar/termsold/")
	link(rel="alternate" hreflang="ch-CH" href="https://royalfut.com/ch/termsold/")
	link(rel="alternate" hreflang="de-DE" href="https://royalfut.com/de/termsold/")
	link(rel="alternate" hreflang="en-US" href="https://royalfut.com/termsold/")
	link(rel="alternate" hreflang="en-GB" href="https://royalfut.com/termsold/")
	link(rel="alternate" hreflang="en-EN" href="https://royalfut.com/termsold/")
	link(rel="alternate" hreflang="es-ES" href="https://royalfut.com/es/termsold/")
	link(rel="alternate" hreflang="fr-FR" href="https://royalfut.com/fr/termsold/")
	link(rel="alternate" hreflang="it-IT" href="https://royalfut.com/it/termsold/")
	link(rel="alternate" hreflang="nl-NL" href="https://royalfut.com/nl/termsold/")
	link(rel="alternate" hreflang="no-NO" href="https://royalfut.com/no/termsold/")
	link(rel="alternate" hreflang="pl-PL" href="https://royalfut.com/pl/termsold/")
	link(rel="alternate" hreflang="pt-PT" href="https://royalfut.com/pt/termsold/")
	link(rel="alternate" hreflang="ru-RU" href="https://royalfut.com/ru/termsold/")
	link(rel="alternate" hreflang="sw-SE" href="https://royalfut.com/sw/termsold/")
	link(rel="alternate" hreflang="tr-TR" href="https://royalfut.com/tr/termsold/")
	link(rel="alternate" hreflang="x-default" href=" https://royalfut.com/termsold/")

block content
	include ../base

	picture.site__media.is-hidden
		img(src=`${baseDir}images/bg-.jpg` alt="bg picture")

	.site.is-hidden(data-barba="container" data-barba-namespace="page")
		include ../../pug/components/header

		main
			.container
				.site-grid
					include ../../pug/pages/ar/termsold
					include ../../pug/components/sidebar

		include ../../pug/components/footer
		include ../../pug/components/cookie

		include ../../pug/components/signin
